<h1>Listing acedemic_years</h1>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>Start date</th>
      <th>End date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @acedemic_years.each do |acedemic_year| %>
      <tr>
        <td><%= acedemic_year.year %></td>
        <td><%= acedemic_year.start_date %></td>
        <td><%= acedemic_year.end_date %></td>
        <td><%= link_to 'Show', acedemic_year %></td>
        <td><%= link_to 'Edit', edit_acedemic_year_path(acedemic_year) %></td>
        <td><%= link_to 'Destroy', acedemic_year, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Acedemic year', new_acedemic_year_path %>
